<h4>Lista de Clientes Morosos</h4>
<table class="table">
  <thead class="thead-light">
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Saldo</th>
  </thead>
  <tbody>
    {{#each clientes}}
    <tr>
      <td scope="col">{{this.nombre}}</td>
      <td scope="col">{{this.apellido}}</td>
      <td scope="col">{{this.saldo}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
